#
# Aliases for bash/zsh
#
#
alias cds="cd ~/scripts"
alias cld="~/scripts/cld.sh $*"
alias cls="clear"
# "Find file within current directory; output: relative path"
alias ff="find -name $1"
alias ga="git add $*"
alias gb="git branch $*"
alias gc="git commit $*"
alias gcl="git config --list --show-origin"
alias gcm="git commit -m $*"
alias gcp="git cherry-pick $*"
alias gd="git diff $*"
alias gdt="git difftool $*"
alias gf="git fetch $*"
alias gl="git log"
alias gln="git log -n$1"
alias glo="git log --oneline"
alias glon="git log --oneline -n$1"
alias glop="git log --oneline -n25"
alias glmy="git log --author='${GIT_AUTHOR_NAME}'"
alias glnmy="git log -n$1 --author='${GIT_AUTHOR_NAME}'"
alias glomy="git log --oneline --author='${GIT_AUTHOR_NAME}'"
alias glonmy="git log --oneline -n$1 --author='${GIT_AUTHOR_NAME}'"
alias glopmy="git log --oneline -n25 --author='${GIT_AUTHOR_NAME}'"
alias gm="git merge $*"
alias gp="git pull $*"
alias gpl="git pull $*"
alias gps="git push $*"
alias gr="git rebase $*"
alias grs="git stash pop"
alias gs="git status $*"
alias gsv="git stash push"
alias gsw="git switch $*"
#alias ip="echo print ip4 address"
#alias ip4="echo print ip4 address"
#alias ip6="echo print ip6 address"
alias ll="ls -la --color $*"
alias lp="ps | grep $1"
alias make="~/scripts/make.sh $*"
alias mi="~/scripts/mi.sh $*"
alias ms="~/scripts/ms.sh $*"
alias rmd="~/scripts/rmd.sh $*"
alias sshr="ssh $1 -R 52698:127.0.0.1:52698"
